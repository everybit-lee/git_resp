import rpy2
import rpy2.robjects as robj

rsp= '''
library(igraph)
nl<-read_graph("E:/pydata/0623/a06.ncol",format="ncol")
wt <- walktrap.community(nl, modularity=TRUE, weights=nl$weight)
'''
pywt=robj.r(rsp)
pywt=robj.r('summary(wt)')
print(pywt)